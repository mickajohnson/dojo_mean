<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Mongoose</title>
  </head>
  <body>
    <form action="/users" method="post">
      <label for="name">
        Name: <input type="text" name="name">
      </label>
      <label for="age">
        Age: <input type="text" name="age">
      </label>
      <input type="submit"value="Submit">
    </form>
      <% for (var x in users) { %>
        <h3>User <%= x %></h3>
        <p>Name: <%= users[x].name %></p>
        <p>Age: <%= users[x].age %></p>
        <hr>
      <% } %>
      <a href="/1955">1955 link</a>
  </body>
</html>
